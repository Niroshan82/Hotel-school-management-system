@using RMS2.Models
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

    <div class="row user">
        <div class="col-md-12">
            <div class="profile">
                <div class="info">
                    <img class="user-img" src="https://s3.amazonaws.com/uifaces/faces/twitter/jsa/128.jpg">
                    <h4>@Session["userName"].ToString()</h4>
                    <p>Hotel-School Admin</p>
                </div>
                <div class="cover-image"></div>
            </div>
        </div>
@foreach (Feedback p in ViewBag.listFeedback)
{
        <div class="col-md-12">
            <div class="tab-content">
                <div class="tab-pane active" id="user-timeline">
                    <div class="timeline-post">
                        <div class="post-media">
                            <div class="content">
                                <h5><a href="#">@p.Name</a></h5>
                                <p class="text-muted"><small>@p.EnteredDate</small></p>
                            </div>
                        </div>
                        <div class="post-content">
                            <p>@p.Comments</p>
                        </div>
                        <div class="post-content">
                            <p>My Rating : @p.Rating</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
}
    </div>

   


@section scripts{
    <script type="text/javascript">
      if(document.location.hostname == 'pratikborsadiya.in') {
